{
  "name": "wp-flawless",
  "version": "0.1.1",
  "description": "Flawless Theme.",
  "repository": {
    "url": "https://github.com/UsabilityDynamics/wp-flawless",
    "type": "git"
  },
  "config": {
    "out": "./components",
    "namespace": "Flawless",
    "basename": "app"
  },
  "build": {
    "scripts": {
      "out": "./public/scripts",
      "settings": {
        "minify": true
      }
    },
    "styles": {
      "out": "./public/styles",
      "settings": {
        "minify": true
      }
    },
    "views": {
      "out": "./public/views"
    },
    "schemas": {
      "out": "./public/schemas"
    },
    "libs": {
      "out": "./core/vendor",
      "settings": {
        "autoload": true
      }
    }
  },
  "scripts": [
    "core/scripts/app.js"
  ],
  "styles": [
    "core/styles/content.less",
    "core/styles/mixins.less",
    "core/styles/app.less"
  ],
  "views": [
    "core/views/template.html"
  ],
  "images": [
    "core/images/ajax-loader.gif",
    "core/images/no-skin-thumbanil.gif",
    "core/images/fancybox-sprite.png",
    "core/images/flawless-default-logo.png",
    "core/images/glyphicons-halflings-white.png",
    "core/images/glyphicons-halflings.png"
  ],
  "dependencies": {
    "usabilitydynamics/lib-api": "*",
    "usabilitydynamics/lib-loader": "*",
    "usabilitydynamics/lib-settings": "*",
    "usabilitydynamics/lib-saas": "*",
    "usabilitydynamics/lib-cdn": "*",

    "usabilitydynamics/js-spa": "*",
    "usabilitydynamics/js-elastic-filter": "*",

    "lauramelos/isotope": "*",
    "components/modernizr": "*",
    "danro/jquery-easing": "*",

    "camshaft/bootstrap-responsive": "*",
    "camshaft/bootstrap-grid": "*",
    "camshaft/bootstrap-reset": "*"
  },
  "license": "MIT"
}
