{
  "name": "discodonniepresents/wp-disco",
  "type": "wordpress-theme",
  "description": "Festival Theme for the Drop Network",
  "author": {
    "name": "Usability Dynamics",
    "url": "http://UsabilityDynamics.com"
  },
  "license": "GPL-2.0",
  "tags": [ "one-column", "two-column", "translation-ready", "responsive", "wordpress", "theme" ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wordpress-theme:fa1942c87f9b2a7314636d520d8697fb0d9447f3@repository.usabilitydynamics.com"
    }
  ],
  "minimum-stability": "dev",
  "config": {
    "preferred-install": "dist",
    "github-protocols": [ "ssh" ],
    "vendor-dir": "vendor",
    "bin-dir": "vendor/bin",
    "component-dir": "components",
    "optimize-autoloader": true
  },
  "require": {
    "composer/installers": "~1.0",
    "UsabilityDynamics/lib-carrington": "~1.3",
    "UsabilityDynamics/lib-wp-theme": "~0.2",
  	"UsabilityDynamics/lib-js-elastic-filter": "~3.0"
  },
  "extra": {
    "installer-paths": {
      "vendor/plugins/{$name}": [
        "type:wordpress-muplugin",
        "type:wordpress-plugin",
        "type:wpackagist-plugin"
      ],
      "vendor/themes/{$name}": [
        "type:wordpress-theme"
      ],
      "vendor/{$vendor}/{$name}": [
        "type:wordpress-library"
      ]
    }
  },
  "require-dev": {
    "UsabilityDynamics/wp-flawless/v1.0": "master"
  }
}
