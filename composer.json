{
  "name": "discodonniepresents/wp-spectacle",
  "type": "wordpress-theme",
  "description": "Spectacle theme for the EDM Network",
  "author": {
    "name": "Usability Dynamics",
    "url": "http://UsabilityDynamics.com"
  },
  "license": "GPL-2.0",
  "license_url": "http://www.gnu.org/licenses/gpl-2.0.html",
  "tags": [ "one-column", "two-column", "translation-ready", "responsive", "wordpress", "theme" ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://repository.usabilitydynamics.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "composer/installers": "~1.0",
    "UsabilityDynamics/lib-wp-theme": "~0.3",
    "UsabilityDynamics/lib-carrington": "~1.2"
  },
  "extra": {
    "installer-paths": {
      "vendor/{$vendor}/{$name}": [
        "type:wordpress-library"
      ],
      "vendor/modules/{$name}": [
        "type:wordpress-muplugin",
        "type:wordpress-plugin",
        "type:wpackagist-plugin",
        "type:wordpress-theme"
      ]
    }
  },
  "autoload": {
    "classmap": [ "lib" ]
  }
}
