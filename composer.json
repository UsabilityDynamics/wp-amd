{
  "name": "usabilitydynamics/wp-site",
  "version": "1.0.0",
  "type": "project",
  "repositories": [
    {
      "type": "composer",
      "url": "http://repository.usabilitydynamics.com"
    }
  ],
  "config": {
    "preferred-install": "source",
    "vendor-dir": "vendor/libraries",
    "bin-dir": "vendor/bin",
    "optimize-autoloader": true,
    "autoloader-suffix": "UsabilityDynamicsVendor"
  },
  "extra": {
    "installer-paths": {
      "vendor/libraries/{$vendor}/{$name}": [
        "type:wordpress-library"
      ],
      "vendor/modules/{$name}": [
        "type:wordpress-muplugin",
        "type:wordpress-module"
      ],
      "vendor/plugins/{$name}": [
        "type:wordpress-plugin"
      ],
      "vendor/themes/{$name}": [
        "type:wordpress-theme"
      ]
    },
    "active-plugins": {
      "w3-total-cache": "0.9.4"
    },
    "settings":{
      "site": {
        "db_prefix": "wp_",
        "db_name": "wordpress",
        "db_user": "root",
        "db_password": "root",
        "db_host": "localhost",
        "wp_debug": true,
        "wp_debug_display": false,
        "wp_debug_log": true,
        "wp_logs_dir": "/var/log",
        "wp_logs_handler": "file",
        "wp_default_theme": "wp-splash",
        "auth_key": "CHANGE-ME",
        "secure_auth_key": "CHANGE-ME",
        "logged_in_key": "CHANGE-ME",
        "nonce_key": "CHANGE-ME",
        "auth_salt": "CHANGE-ME",
        "secure_auth_salt": "CHANGE-ME",
        "logged_in_salt": "CHANGE-ME",
        "nonce_salt": "CHANGE-ME",
        "secret_key": "CHANGE-ME",
        "force_ssl_admin": false
      },
      "default": {
        "wp_memory_limit": "512M",
        "wp_max_memory_limit": "{wp_memory_limit}",
        "admin_cookie_path": "/manage",
        "sitecookiepath": "/manage",
        "wp_ssl_port": 443,
        "db_charset": "utf8",
        "db_collate": "utf8_general_ci",
        "disallow_file_edit": false,
        "disallow_file_mods": false,
        "wp_post_revisions": 10,
        "empty_trash_days": 7,
        "disable_wp_cron": "true",
        "script_debug": false,
        "savequeries": true,
        "wp_use_themes": true,
        "wplang": "en_US"
      },
      "paths": {
        "wp_content_dir": "{wp_base_dir}",
        "wp_content_url": "{wp_base_url}",
        "wp_veneer_storage": "storage",
        "wp_veneer_public": "{wp_veneer_storage}/public/{wp_base_domain}",
        "wp_veneer_theme_dir": "{wp_veneer_storage}/themes",
        "wp_vendor_dir": "{wp_base_dir}/vendor",
        "wp_vendor_url": "{wp_base_url}/vendor",
        "wp_plugin_dir": "{wp_vendor_dir}/plugins",
        "wp_plugin_url": "{wp_vendor_url}/plugins",
        "wp_module_dir": "{wp_vendor_dir}/modules",
        "wp_module_url": "{wp_vendor_url}/modules",
        "wpmu_plugin_dir": "{wp_vendor_dir}/libraries",
        "wpmu_plugin_url": "{wp_vendor_url}/libraries",
        "wp_theme_dir": "{wp_vendor_dir}/themes",
        "wp_theme_url": "{wp_vendor_url}/themes",
        "wp_system_directory": "{wp_vendor_dir}/libraries/automattic/wordpress",
        "wp_siteurl": "{wp_vendor_url}/libraries/automattic/wordpress",
        "uploadblogsdir": "{wp_veneer_public}",
        "uploads": "{uploadblogsdir}/media",
        "bloguploaddir": "{wp_content_dir}/{uploads}",
        "cookie_domain": "{wp_base_domain}",
        "w3tc_dir": "{wp_plugin_dir}/w3-total-cache"
      }
    }
  },
  "require": {
    "php": ">=5.3",
    "usabilitydynamics/lib-installer": "master",
    "automattic/wordpress": "3.9.1",
    "usabilitydynamics/wp-cluster": "master",
    "usabilitydynamics/wp-veneer": "master",
    "usabilitydynamics/wp-splash": "master"
  },
  "autoload": {
    "classmap": [
      "application/lib"
    ],
    "files": [
      "application/bootstrap.php",
      "vendor/modules/wp-cluster/wp-cluster.php",
      "vendor/modules/wp-veneer/wp-veneer.php"
    ]
  },
  "minimum-stability": "dev",
  "license": "GPLv2"
}