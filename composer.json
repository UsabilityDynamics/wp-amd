{
  "name": "usabilitydynamics/wp-splash",
  "version": "0.1.0",
  "description": "Wordpress Splash Theme.",
  "author": {
    "name": "Usability Dynamics",
    "url": "http://UsabilityDynamics.com"
  },
  "license": "GPL-2.0",
  "license_url": "http://www.gnu.org/licenses/gpl-2.0.html",
  "tags": [
    "one-column",
    "two-column",
    "translation-ready",
    "responsive",
    "wordpress",
    "theme"
  ],
  "config": {
    "vendor-dir": "vendor/libraries",
    "bin-dir": "vendor/bin",
    "optimize-autoloader": true
  },
  "extra": {
    "installer-name": "wp-splash",
    "installer-paths": {
      "vendor/libraries/{$name}": [
        "type:ibrary"
      ],
      "vendor/modules/{$name}": [
        "type:wordpress-modules"
      ],
      "vendor/plugins/{$name}": [
        "type:wordpress-plugin"
      ],
      "vendor/themes/{$name}": [
        "type:wordpress-theme"
      ]
    }
  },
  "require": {
    "php": ">=5.3",
    "matthiasmullie/minify": "master",
    "zaininnari/html-minifier": "master",
    "siteorigin/panels": "master",
    "usabilitydynamics/lib-installer": "master",
    "usabilitydynamics/lib-layout-engine": "master",
    "usabilitydynamics/lib-ui": "master"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UsabilityDynamics/wp-splash.git"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://repository.usabilitydynamics.com"
    }
  ],
  "minimum-stability": "dev"
}
