{
  "name": "flawless",
  "version": "0.1.1",
  "type": "wordpress-theme",
  "homepage": "http://github.com/UsabilityDynamics/wp-flawless",
  "minimum-stability": "dev",
  "keywords": [
    "theme", "responsive", "bootstrap"
  ],
  "license": [
    "GPL-3.0+"
  ],
  "authors": [
    {
      "name": "Maxim Peshkov",
      "email": "maxim.peshkov@usabilitydynamics.com",
      "homepage": "http://usabilitydynamics.com",
      "role": "Developer"
    },
    {
      "name": "Anton Korotkov",
      "email": "anton.korotkov@usabilitydynamics.com",
      "homepage": "http://usabilitydynamics.com",
      "role": "Developer"
    }
  ],
  "config": {
    "vendor-dir": "./vendor",
    "bin-dir": "./core/bin"
  },
  "repositories": [
    {
      "type":"composer",
      "url":"http://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "ud/wp-libs",
        "version": "trunk",
        "source": {
          "url": "http://github.com/UsabilityDynamics/wp-libs",
          "type": "svn",
          "reference": "trunk/src"
        }
      }
    }
  ],
  "require": {
    "ud/wp-libs": "*",
    "justinrainbow/json-schema": "~1.3"
  }
}
