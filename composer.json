{
  "name": "usabilitydynamics/wp-splash",
  "description": "Wordpress Splash Theme.",
  "type"   : "wordpress-theme",
  "license": "GPL-2.0",
  "license_url": "http://www.gnu.org/licenses/gpl-2.0.html",
  "tags": [
    "one-column",
    "two-column",
    "translation-ready",
    "responsive",
    "wordpress",
    "theme"
  ],
  "config": {
    "vendor-dir": "vendor/libraries",
    "bin-dir": "vendor/bin",
    "optimize-autoloader": true
  },
  "extra": {
    "installer-name": "wp-splash",
    "installer-paths": {
      "vendor/libraries/{$name}": [
        "type:ibrary"
      ],
      "vendor/modules/{$name}": [
        "type:wordpress-modules"
      ],
      "vendor/plugins/{$name}": [
        "type:wordpress-plugin"
      ],
      "vendor/themes/{$name}": [
        "type:wordpress-theme"
      ]
    }
  },
  "require": {
    "php": ">=5.3",
    "zaininnari/html-minifier": "*",
    "composer/installers": "*",
    "usabilitydynamics/lib-ui": "*",
    "siteorigin/panels": "master"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://www.usabilitydynamics.com/repository"
    },
    {
      "type":"composer",
      "url":"http://wpackagist.org"
    }
  ],
  "minimum-stability": "dev"
}
