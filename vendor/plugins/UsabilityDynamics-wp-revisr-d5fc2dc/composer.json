{
  "name": "usabilitydynamics/wp-revisr",
  "description": "WordPress repository manager",
  "type": "wordpress-plugin",
  "homepage": "https://github.com/UsabilityDynamics/wp-revisr",
  "keywords": [
    "wordpress",
    "git"
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Andy Potanin",
      "homepage": "http://usabilitydynamics.com"
    }
  ],
  "repositories": [
    {
      "type":"composer",
      "url":"http://wpackagist.org"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "require": {
    "php": ">=5.3",
    "composer/installers": "~1.0"
  }
}
