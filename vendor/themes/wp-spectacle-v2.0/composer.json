{
  "name": "discodonniepresents/wp-spectacle",
  "type": "wordpress-theme",
  "description": "Spectacle theme for the EDM Network",
  "author": {
    "name": "Usability Dynamics",
    "url": "http://UsabilityDynamics.com"
  },
  "license": "GPL-2.0",
  "license_url": "http://www.gnu.org/licenses/gpl-2.0.html",
  "tags": [ "one-column", "two-column", "translation-ready", "responsive", "wordpress", "theme" ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://repository.usabilitydynamics.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "UsabilityDynamics/lib-wp-theme": "~0.3",
    "UsabilityDynamics/siteorigin-panels": "master",
    "mustache/mustache": "~2.6",
    "kertz/twitteroauth": "dev-master"
  },
  "autoload": {
    "classmap": [ "lib" ]
  },
  "config": {
    "preferred-install": "dist",
    "github-protocols": [ "ssh" ],
    "vendor-dir": "vendor",
    "bin-dir": "vendor/bin",
    "optimize-autoloader": true
  },
  "extra": {
    "installer-paths": {
      "vendor/themes/{$name}": [
        "type:wordpress-theme"
      ],
      "vendor/{$vendor}/{$name}": [
        "usabilitydynamics/lib-meta-box",
        "type:wpackagist-plugin",
        "type:wordpress-muplugin",
        "type:wordpress-plugin",
        "type:wordpress-module",
        "type:wordpress-library"
      ]
    }
  }
}
