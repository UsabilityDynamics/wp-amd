{
  "name": "discodonniepresents/wp-festival",
  "version": "2.0.0",
  "type": "wordpress-theme",
  "description": "Festival Theme for the Drop Network",
  "author": {
    "name": "Usability Dynamics",
    "url": "http://UsabilityDynamics.com"
  },
  "license": "GPL-2.0",
  "tags": [ "one-column", "two-column", "translation-ready", "responsive", "wordpress", "theme" ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://repository.usabilitydynamics.com"
    },
    {
      "type": "package",
      "package": {
        "name": "components/malihu-custom-scrollbar-plugin",
        "type": "component",
        "version": "3.0.3",
        "dist": {
          "url": "https://github.com/malihu/malihu-custom-scrollbar-plugin/archive/3.0.3.zip",
          "type": "zip"
        },
        "source": {
          "url": "https://github.com/malihu/malihu-custom-scrollbar-plugin",
          "type": "git",
          "reference": "3.0.3"
        },
        "extra": {
          "component": {
            "scripts": [
              "js/minified/jquery.mCustomScrollbar.min.js"
            ]
          }
        },
        "require": {
          "robloach/component-installer": "*"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "components/imagesloaded",
        "type": "component",
        "version": "3.1.8",
        "dist": {
          "url": "https://github.com/desandro/imagesloaded/archive/v3.1.8.zip",
          "type": "zip"
        },
        "source": {
          "url": "https://github.com/desandro/imagesloaded",
          "type": "git",
          "reference": "3.1.8"
        },
        "extra": {
          "component": {
            "scripts": [
              "imagesloaded.pkgd.js"
            ]
          }
        },
        "require": {
          "robloach/component-installer": "*"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "components/fitvids",
        "type": "component",
        "version": "1.1.0",
        "dist": {
          "url": "https://github.com/davatron5000/FitVids.js/archive/v1.1.0.zip",
          "type": "zip"
        },
        "source": {
          "url": "https://github.com/davatron5000/FitVids.js",
          "type": "git",
          "reference": "1.1.0"
        },
        "extra": {
          "component": {
            "scripts": [
              "jquery.fitvids.js"
            ]
          }
        },
        "require": {
          "robloach/component-installer": "*"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "components/jquery-selectBoxIt",
        "type": "component",
        "version": "3.8.0",
        "dist": {
          "url": "https://github.com/gfranko/jquery.selectBoxIt.js/archive/3.8.0.zip",
          "type": "zip"
        },
        "source": {
          "url": "https://github.com/gfranko/jquery.selectBoxIt.js",
          "type": "git",
          "reference": "3.8.0"
        },
        "extra": {
          "component": {
            "scripts": [
              "src/javascripts/jquery.selectBoxIt.js"
            ]
          }
        },
        "require": {
          "robloach/component-installer": "*"
        }
      }
    }
  ],
  "config": {
    "preferred-install": "source",
    "github-protocols": [ "ssh" ],
    "vendor-dir": "vendor/libraries",
    "bin-dir": "vendor/bin",
    "component-dir": "static/scripts/src/components",
    "optimize-autoloader": true,
    "component": {
      "name": "app",
      "out": "static/scripts/app.js",
      "baseUrl": "static/scripts/src",
      "mainConfigFile": "static/scripts/src/components/require.config.js",
      "paths": {
        "jquery": "lib/jquery"
      },
      "uglify": {
        "no_mangle": true
      },
      "shim": {
        "third-party/imagelightbox.min": {
          "deps": [ "jquery" ]
        },
        "components/dotdotdot/dotdotdot-built" : {
          "deps": [ "jquery" ]
        },
        "'components/sticky/jquery.sticky": {
          "deps": [ "jquery" ]
        },
        "third-party/imagelightbox": {
          "deps": [ "jquery" ]
        },
        "components/fancybox2/fancybox2-built": {
          "deps": [ "jquery" ]
        },
        "components/fitvids/fitvids-built": {
          "deps": [ "jquery" ]
        },
        "components/jquery-selectBoxIt/jquery-selectBoxIt-built": {
          "deps": [ "jquery", "jquery.ui" ]
        },
        "third-party/jquery-ui-1.11.1.custom/jquery-ui.min": {
          "deps": [ "jquery" ]
        }
      }
    }
  },
  "minimum-stability": "dev",
  "require": {
    "composer/installers": "~1.0",
    "usabilitydynamics/lib-wp-theme": "master",
    "usabilitydynamics/lib-settings": "master",
    "usabilitydynamics/lib-carrington": "master",
    "usabilitydynamics/lib-wp-shortcode": "master",
    "usabilitydynamics/lib-meta-box": "master",
    "moment/moment": "~2.8",
    "components/jquery-selectBoxIt": "~3.8",
    "components/knockout": "3.1.0",
    "components/iscroll": "5.1.1",
    "components/dotdotdot": "1.6.14",
    "components/sticky": "1.0.0",
    "components/masonry": "3.1.5",
    "components/imagesloaded": "3.1.8",
    "components/fitvids": "1.1.0",
    "components/fancybox2": "2.1.5",
    "components/malihu-custom-scrollbar-plugin": "3.x",
    "components/jquery": "~2.1"
  },
  "autoload": {
    "classmap": [ "lib" ]
  },
  "extra": {
    "installer-name": "wp-festival-2",
    "installer-paths": {
      "vendor/libraries/{$vendor}/{$name}": [
        "type:wordpress-library"
      ],
      "vendor/modules/{$name}": [
        "type:wordpress-muplugin",
        "type:wordpress-module"
      ],
      "vendor/plugins/{$name}": [
        "type:wordpress-plugin",
        "type:wpackagist-plugin"
      ],
      "vendor/themes/{$name}": [
        "type:wordpress-theme"
      ]
    },
    "settings": {
      "applicationName": "wp-festival",
      "raygunKey": "vJ/vlKJh1uXtGJ6TJbDnVw=="
    }
  }
}

