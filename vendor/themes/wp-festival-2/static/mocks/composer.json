{
  "name": "discodonniepresents/wp-festival-mock",
  "version": "0.0.1",
  "type": "mockups",
  "description": "Festival Theme v2 Mockups",
  "author": {
    "name": "Usability Dynamics",
    "url": "http://UsabilityDynamics.com"
  },
  "license": "GPL-2.0",
  "repositories": [
    {
      "type": "composer",
      "url": "http://repository.usabilitydynamics.com"
    },
    {
      "type": "package",
      "package": {
        "name": "components/knockout",
        "type": "component",
        "version": "3.1.0",
        "dist": {
            "url": "https://github.com/knockout/knockout/archive/v3.1.0.zip",
            "type": "zip"
        },
        "source": {
            "url": "https://github.com/knockout/knockout",
            "type": "git",
            "reference": "3.1.0"
        },
        "extra": {
          "component": {
              "scripts": [
                  "dist/knockout.js"
              ]
          }
        },
        "require": {
            "robloach/component-installer": "*"
        }
      }
    },
	  {
		  "type": "package",
		  "package": {
			  "name": "components/iscroll",
			  "type": "component",
			  "version": "5.1.1",
			  "dist": {
				  "url": "https://github.com/cubiq/iscroll/archive/v5.1.1.zip",
				  "type": "zip"
			  },
			  "source": {
				  "url": "https://github.com/cubiq/iscroll",
				  "type": "git",
				  "reference": "5.1.1"
			  },
			  "extra": {
				  "component": {
					  "scripts": [
						  "build/iscroll.js"
					  ]
				  }
			  },
			  "require": {
				  "robloach/component-installer": "*"
			  }
		  }
	  },
    {
      "type": "package",
      "package": {
        "name": "components/dotdotdot",
        "type": "component",
        "version": "1.6.14",
        "dist": {
          "url": "https://github.com/BeSite/jQuery.dotdotdot/archive/v1.6.14.zip",
          "type": "zip"
        },
        "source": {
          "url": "https://github.com/BeSite/jQuery.dotdotdot",
          "type": "git",
          "reference": "1.6.14"
        },
        "extra": {
          "component": {
            "scripts": [
              "src/js/jquery.dotdotdot.min.js"
            ]
          }
        },
        "require": {
          "robloach/component-installer": "*"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "components/sticky",
        "type": "component",
        "version": "1.0.0",
        "dist": {
          "url": "https://github.com/2degrees/jquery.sticky/archive/master.zip",
          "type": "zip"
        },
        "source": {
          "url": "https://github.com/2degrees/jquery.sticky",
          "type": "git",
          "reference": "1.0.0"
        },
        "extra": {
          "component": {
            "scripts": [
              "jquery.sticky.js"
            ]
          }
        },
        "require": {
          "robloach/component-installer": "*"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "components/masonry",
        "type": "component",
        "version": "3.1.5",
        "dist": {
          "url": "https://github.com/desandro/masonry/archive/v3.1.5.zip",
          "type": "zip"
        },
        "source": {
          "url": "https://github.com/desandro/masonry",
          "type": "git",
          "reference": "3.1.5"
        },
        "extra": {
          "component": {
            "scripts": [
              "dist/masonry.pkgd.min.js"
            ]
          }
        },
        "require": {
          "robloach/component-installer": "*"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "components/caroufredsel",
        "type": "component",
        "version": "6.2.1",
        "dist": {
          "url": "https://github.com/gilbitron/carouFredSel/archive/master.zip",
          "type": "zip"
        },
        "source": {
          "url": "https://github.com/gilbitron/carouFredSel",
          "type": "git",
          "reference": "6.2.1"
        },
        "extra": {
          "component": {
            "scripts": [
              "jquery.carouFredSel-6.2.1.js"
            ]
          }
        },
        "require": {
          "robloach/component-installer": "*"
        }
      }
    }


  ],
  "config": {
    "vendor-dir": "vendor/libraries",
    "bin-dir": "vendor/bin",
    "component-dir": "vendor/components",
    "optimize-autoloader": true,
    "autoloader-suffix": "VendorPackages",
    "component": {
      "baseUrl": "components",
      "mainConfigFile": "vendor/components/require.config.js",
      "baseUrl": "application/static/scripts/src",
      "paths": {
        "components": "../../../../vendor/components",
        "jquery": "../../../../vendor/components/jquery/jquery"
      },
      "shim": {
        "components/dotdotdot" : {
          "deps": ["jquery"],
          "exports": "dotdotdot"
        },
        "components/stickem": {
          "deps": ["jquery"],
          "exports": "stickem"
        }
      }
    }
  },
  "minimum-stability": "dev",
  "require": {
    "composer/installers": "~1.0",
    "components/jquery": "2.1.0",
    "components/knockout": "3.1.0",
	  "components/iscroll": "5.1.1",
    "components/dotdotdot": "1.6.14",
    "components/sticky": "1.0.0",
    "components/masonry": "3.1.5",
    "components/caroufredsel": "6.2.1"
  },
  "autoload": {
  },
  "scripts": {
    "pre-install-cmd": [
      "npm install --silent"
    ],
    "post-install-cmd": [
      "grunt deploy --force"
    ]
  }
}
