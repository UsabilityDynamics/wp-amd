{
  "id": "event",
  "type": "schema",
  "title": "Event",
  "revision": 2,
  "schema": "http://api.wpelastic.io/schema/v1/event",
  "types": {
    "Event": {
      "data": {
        "capability_type": "page",
        "label": "Event",
        "labels": {
          "add_new": "New Event",
          "add_new_item": "Add New Event",
          "all_items": "All Events",
          "edit_item": "Edit Event",
          "menu_name": "Events",
          "name": "Events",
          "not_found": "No Events found",
          "not_found_in_trash": "No Events in Trash",
          "parent_item_colon": "Parent Event:",
          "search_items": "Search Events",
          "singular_name": "Event",
          "update_item": "Update Event",
          "view_item": "View Event"
        },
        "description": "Event",
        "can_export": true,
        "exclude_from_search": false,
        "has_archive": true,
        "hierarchical": true,
        "menu_icon": "dashicons-calendar",
        "menu_position": 20,
        "public": true,
        "publicly_queryable": true,
        "show_in_admin_bar": true,
        "show_in_menu": true,
        "show_in_nav_menus": false,
        "show_ui": true,
        "supports": [
          "title",
          "editor",
          "thumbnail",
          "comments",
          "post-formats",
          "revisions"
        ]
      },
      "taxonomies": [
        "age-limit",
        "genre",
        "event-type"
      ],
      "meta": {
        "information": {
          "fields": [
            "dateStart",
            "dateEnd",
            "urlTicket",
            "urlRsvp"
          ]
        },
        "event_meta": {
          "fields": [
            "termCategory",
            "termTag",
            "termGenre",
            "termEventType",
            "termAgeLimit"
          ]
        },
        "related": {
          "fields": [
            "relatedTour",
            "relatedPromoters",
            "relatedVenues",
            "relatedArtists"
          ]
        },
        "images": {
          "fields": [
            "posterImages"
          ]
        }
      }
    }
  },
  "meta": {
    "dateStart": {
      "name": "Starts",
      "type": "datetime",
      "_desc": "Date and time this event begins.",
      "std": "2014-05-06 00:00",
      "js_options": {},
      "options": {
      }
    },
    "dateEnd": {
      "name": "Ends",
      "type": "datetime",
      "_desc": "Date and time this event ends."
    },
    "urlTicket": {
      "id": "urlTicket",
      "name": "Tickets URL",
      "_desc": "The link for purchasing tickets",
      "type": "url"
    },
    "urlRsvp": {
      "id": "urlRsvp",
      "name": "RSVP URL",
      "type": "url"
    },
    "termTag": {
      "id": "termTag",
      "name": "Tags",
      "multiple": true,
      "type": "taxonomy",
      "options": {
        "taxonomy": "post_tag",
        "type": "select_advanced",
        "args": {
          "orderby": "count"
        }
      }
    },
    "termCategory": {
      "id": "termCategory",
      "name": "Category",
      "multiple": true,
      "type": "taxonomy",
      "options": {
        "taxonomy": "category",
        "type": "select_advanced",
        "args": {
          "orderby": "count"
        }
      }
    },
    "termGenre": {
      "name": "Genre",
      "id": "termGenre",
      "type": "taxonomy",
      "options": {
        "taxonomy": "genre",
        "type": "checkbox_list",
        "args": {}
      }
    },
    "termAgeLimit": {
      "name": "Age Limit",
      "id": "termAgeLimit",
      "type": "taxonomy",
      "options": {
        "taxonomy": "age-limit",
        "type": "checkbox_list",
        "args": {}
      }
    },
    "termEventType": {
      "name": "Type",
      "id": "termEventType",
      "type": "taxonomy",
      "options": {
        "taxonomy": "event-type",
        "type": "checkbox_list",
        "args": {}
      }
    },
    "relatedArtists": {
      "id": "artists",
      "name": "Artists",
      "type": "post",
      "post_type": "artist",
      "_desc": "Lineup.",
      "field_type": "select_advanced",
      "multiple": true,
      "placeholder": "Select Artists",
      "query_args": {
        "post_status": "publish",
        "posts_per_page": -1
      }
    },
    "relatedPromoters": {
      "id": "promoters",
      "name": "Promoter",
      "type": "post",
      "post_type": "promoter",
      "field_type": "select_advanced",
      "multiple": false,
      "placeholder": "Select Promoters",
      "query_args": {
        "post_status": "publish",
        "posts_per_page": -1
      }
    },
    "relatedVenues": {
      "id": "venues",
      "name": "Venue",
      "type": "post",
      "post_type": "venue",
      "field_type": "select_advanced",
      "multiple": false,
      "placeholder": "Select Venues",
      "query_args": {
        "post_status": "publish",
        "posts_per_page": 20
      }
    },
    "relatedTour": {
      "id": "tours",

      "name": "Tour",
      "type": "post",
      "post_type": "tour",
      "field_type": "checkbox_list",
      "multiple": false,
      "placeholder": "Select Tour",
      "query_args": {
        "post_status": "publish",
        "posts_per_page": -1
      }
    },
    "divider": {
      "type": "divider"
    },
    "posterImages": {
      "name": "Poster Images",
      "type": "plupload_image"
    }
  },
  "taxonomies": {
    "event-type": {
      "label": "Type",
      "hierarchical": false,
