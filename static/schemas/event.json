  "types": {
    "Event": {
      "data": {
        "capability_type": "page",
        "label": "Event",
        "labels": {
          "add_new": "New Event",
          "add_new_item": "Add New Event",
          "all_items": "All Events",
          "edit_item": "Edit Event",
          "menu_name": "Events",
          "name": "Events",
          "not_found": "No Events found",
          "not_found_in_trash": "No Events in Trash",
          "parent_item_colon": "Parent Event:",
          "search_items": "Search Events",
          "singular_name": "Event",
          "update_item": "Update Event",
          "view_item": "View Event"
        },
        "description": "Event",
        "can_export": true,
        "exclude_from_search": false,
        "has_archive": true,
        "hierarchical": true,
        "menu_icon": "dashicons-calendar",
        "menu_position": 20,
        "public": true,
        "publicly_queryable": true,
        "show_in_admin_bar": true,
        "show_in_menu": true,
        "show_in_nav_menus": false,
        "show_ui": true,
        "supports": [
          "title",
          "editor",
          "thumbnail",
          "comments",
          "post-formats",
          "revisions"
        ]
      },
      "taxonomies": [
        "age_restriction",
        "genre",
        "event-type"
      ],
      "meta": {
        "information": {
          "fields": [
            "start_date",
            "end_date",
            "url.ticket",
            "url.rsvp"
          ]
        },
        "event_meta": {
          "fields": [
            "term.category",
            "terms.tag",
            "term.genre",
            "term.event_type",
            "term.age_restriction"
          ]
        },
        "related": {
          "fields": [
            "related-tour",
            "related-promoters",
            "related-venues",
            "divider",
            "artists.featured",
            "related-artists"
          ]
        },
        "Images": {
          "fields": [
            "poster-images"
          ]
        }
      }
    }
  },
  "meta": {
    "start_date": {
      "name": "Starts",
      "type": "datetime",
      "_desc": "Date and time this event begins.",
      "std": "2014-05-06 00:00",
      "js_options": {},
      "options": {

      }
    },
    "end_date": {
      "name": "Ends",
      "type": "datetime",
      "_desc": "Date and time this event ends."
    },
    "url.ticket": {
      "id": "url.ticket",
      "name": "Tickets URL",
      "_desc": "The link for purchasing tickets",
      "type": "url"
    },
    "url.rsvp": {
      "id": "url.rsvp",
      "name": "RSVP URL",
      "type": "url"
    },
    "terms.tag": {
      "id": "terms.tag",
      "name": "Tags",
      "multiple": true,
      "type": "taxonomy",
      "options": {
        "taxonomy": "post_tag",
        "type": "select_advanced",
        "args": {
          "orderby": "count"
        }
      }
    },
    "term.category": {
      "id": "term.category",
      "name": "Category",
      "multiple": true,
      "type": "taxonomy",
      "options": {
        "taxonomy": "category",
        "type": "select_advanced",
        "args": {
          "orderby": "count"
        }
      }
    },
    "term.genre": {
      "name": "Genre",
      "id": "term.genre",
      "type": "taxonomy",
      "options": {
        "taxonomy": "genre",
        "type": "checkbox_list",
        "args": {}
      }
    },
    "term.age_restriction": {
      "name": "Age Restriction",
      "id": "term.age_restriction",
      "type": "taxonomy",
      "options": {
        "taxonomy": "age_restriction",
        "type": "checkbox_list",
        "args": {}
      }
    },
    "term.event_type": {
      "name": "Type",
      "id": "term.event_type",
      "type": "taxonomy",
      "options": {
        "taxonomy": "event-type",
        "type": "checkbox_list",
        "args": {}
      }
    },
    "related-artists": {
      "id": "artists",
      "name": "Artists",
      "type": "post",
      "post_type": "artist",
      "_desc": "Lineup.",
      "field_type": "select_advanced",
      "multiple": true,
      "placeholder": "Select Artists",
      "query_args": {
        "post_status": "publish",
        "posts_per_page": -1
      }
    },
    "related-promoters": {
      "id": "promoters",
      "name": "Promoter",
      "type": "post",
      "post_type": "promoter",
      "field_type": "select_advanced",
      "multiple": false,
      "placeholder": "Select Promoters",
      "query_args": {
        "post_status": "publish",
        "posts_per_page": -1
      }
    },
    "related-venues": {
      "id": "venues",
      "name": "Venue",
      "type": "post",
      "post_type": "venue",
      "field_type": "select_advanced",
      "multiple": false,
      "placeholder": "Select Venues",
      "query_args": {
        "post_status": "publish",
        "posts_per_page": 20
      }
    },
    "related-tour": {
      "id": "tours",

      "name": "Tour",
      "type": "post",
      "post_type": "tour",
      "field_type": "checkbox_list",
      "multiple": false,
      "placeholder": "Select Tour",
      "query_args": {
        "post_status": "publish",
        "posts_per_page": -1
      }
    },
    "divider": {
      "type": "divider"
    },
    "poster-images": {
      "name": "Poster Images",
      "type": "plupload_image"
    }
  },
  "taxonomies": {
